{

  "update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "minimum_chrome_version": "28",
  "name": "Bing Search Keyboard Shortcuts",
  "description": "Adds keyboard shortcuts/hotkeys back to Bing search results",
  "version": "1.1.16",
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "author": "GÃ¼nther Bosch <guebosch@gmail.com>",
  "options_ui": {
      "page": "options.html",
      "chrome_style": true
  },
  "permissions": [
      "storage"
  ],
  "content_scripts": [{
    "matches": [
      "*://*.bing.com/*"
    ],
    "js": [
      "js/utils.js",
      "js/shortcuts.js"
    ],
    "css": ["css/highlight.css"]
  }]
}
